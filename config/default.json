{
  "server": {
    "options": {
      "debug": {
        "log": ["error"],
        "request": ["error"]
      }
    },
    "connection": {
      "host": "localhost",
      "port": "3001"
    }
  },

  "engine": {
    "swig": {
      "cache": false,
      "varControls": ["<$", "$>"],
      "tagControls": ["<%", "%>"],
      "cmtControls": ["<#", "#>"]
    }
  },

  "winston": {
    "console": {
      "timestamp": true,
      "level": "debug"
    }
  },

  "locals": {
    "production": false,
    "cdn": "/assets/"
  }

}